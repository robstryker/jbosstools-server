<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="serverSubsystem" name="org.jboss.ide.eclipse.as.wtp.core.server.behavior.serverSubsystem" schema="schema/serverSubsystem.exsd"/>
   <extension-point id="serverProfile" name="org.jboss.ide.eclipse.as.wtp.core.server.behavior.serverProfile" schema="schema/serverProfile.exsd"/>
   <extension-point id="serverBeanTypeProvider" name="serverBeanTypeProvider" schema="schema/serverBeanTypeProvider.exsd"/>
   <extension-point id="serverExportParticipant" name="org.jboss.ide.eclipse.as.wtp.core.server.serverExportParticipant" schema="schema/serverExportParticipant.exsd"/>
   <extension-point id="serverImportParticipant" name="org.jboss.ide.eclipse.as.wtp.core.server.serverImportParticipant" schema="schema/serverImportParticipant.exsd"/>
   <extension
         point="org.eclipse.wst.common.modulecore.referenceResolver">
      <resolver
            class="org.jboss.ide.eclipse.as.wtp.core.vcf.OutputFolderReferenceResolver"
            id="org.jboss.ide.eclipse.as.wtp.core.vcf.outputFolderReferenceResolver">
      </resolver>
      <resolver
            class="org.jboss.ide.eclipse.as.wtp.core.vcf.ExportedClassFolderReferenceResolver"
            id="org.jboss.ide.eclipse.as.wtp.core.vcf.exportedClasspathFolderReferenceResolver">
      </resolver>
   </extension>
   <extension
         point="org.eclipse.wst.common.modulecore.flattenParticipantProvider">
      <flattenParticipantProvider
            class="org.jboss.ide.eclipse.as.wtp.core.vcf.JBTHeirarchyParticipantProvider"
            weight="1">
      </flattenParticipantProvider>
   </extension>
   <extension
         point="org.eclipse.wst.common.modulecore.heirarchyFlattenParticipant">
      <heirarchyParticipant
            class="org.jboss.ide.eclipse.as.wtp.core.vcf.JBTHeirarchyParticipant">
      </heirarchyParticipant>
   </extension>
   <extension
         point="org.jboss.ide.eclipse.as.wtp.core.serverBeanTypeProvider">
      <serverBeanTypeProvider
            class="org.jboss.ide.eclipse.as.core.server.bean.JBossServerBeanTypeProvider"
            weight="10">
      </serverBeanTypeProvider>
   </extension>
   <extension
         point="org.jboss.ide.eclipse.as.wtp.core.serverImportParticipant">
      <importParticipant
            class="org.jboss.ide.eclipse.as.wtp.core.server.export.internal.RuntimePropertyImporter"
            id="org.jboss.ide.eclipse.as.wtp.core.server.export.internal.RuntimePropertyImporter"
            weight="100">
      </importParticipant>
      <importParticipant
            class="org.jboss.ide.eclipse.as.wtp.core.server.export.internal.ServerPropertyImporter"
            id="org.jboss.ide.eclipse.as.wtp.core.server.export.internal.ServerPropertyImporter"
            weight="90">
      </importParticipant>
      <importParticipant
            class="org.jboss.ide.eclipse.as.wtp.core.server.export.internal.ServerLaunchPropertyImporter"
            id="org.jboss.ide.eclipse.as.wtp.core.server.export.internal.ServerLaunchPropertyImporter"
            weight="80">
      </importParticipant>
   </extension>
   <extension
         point="org.jboss.ide.eclipse.as.wtp.core.serverExportParticipant">
      <exportParticipant
            class="org.jboss.ide.eclipse.as.wtp.core.server.export.internal.RuntimePropertyExporter"
            id="org.jboss.ide.eclipse.as.wtp.core.server.export.internal.RuntimePropertyExporter"
            weight="100">
      </exportParticipant>
      <exportParticipant
            class="org.jboss.ide.eclipse.as.wtp.core.server.export.internal.ServerPropertyExporter"
            id="org.jboss.ide.eclipse.as.wtp.core.server.export.internal.ServerPropertyExporter"
            weight="90">
      </exportParticipant>
      <exportParticipant
            class="org.jboss.ide.eclipse.as.wtp.core.server.export.internal.ServerLaunchPropertyExporter"
            id="org.jboss.ide.eclipse.as.wtp.core.server.export.internal.ServerLaunchPropertyExporter"
            weight="80">
      </exportParticipant>
   </extension>
   
   
</plugin>
